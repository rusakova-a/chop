{
  "name": "server",
  "version": "0.1.0",
  "type": "commonjs",
  "scripts": {
    "dev": "tsc && concurrently --kill-others \"npm run dev:tsc\" \"npm run dev:nodemon\"",
    "dev:tsc": "tsc --watch",
    "dev:nodemon": "npx nodemon dist/index.js",
    "start": "tsc && node -r tsconfig-paths/register ./dist/index.js",
    "pm2:start": "pm2 start --name \"server-express\"",
    "pm2:restart": "pm2 restart \"server-express\""
  },
  "dependencies": {
    "cors": "2.8.5",
    "dotenv": "16.4.7",
    "express": "4.21.2",
    "pg": "8.13.1",
    "pg-hstore": "2.3.4",
    "sequelize": "6.37.5",
    "sequelize-typescript": "2.1.6"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/node": "20.17.16",
    "concurrently": "9.1.2",
    "nodemon": "3.1.9",
    "prettier": "3.4.2",
    "typescript": "5.7.3"
  }
}